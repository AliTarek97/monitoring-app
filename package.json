{
    "name": "bosta-backend-challenge",
    "version": "0.1.0",
    "repository": {
        "type": "git"
    },
    "license": "MIT",
    "scripts": {
        "build": "tsc && npm run lint",
        "debug": "npm run build && npm run watch-debug",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "serve-debug": "nodemon --inspect dist/server.js",
        "serve": "node dist/server.js",
        "start": "npm run serve",
        "test": "jest --forceExit --coverage --verbose",
        "watch-node": "nodemon dist/server.js",
        "watch-test": "npm run test -- --watchAll",
        "watch-ts": "tsc -w"
    },
    "dependencies": {
        "@types/mongodb": "3.6.5",
        "async": "3.2.0",
        "axios": "^0.24.0",
        "bcryptjs": "^2.4.3",
        "bluebird": "3.7.2",
        "body-parser": "1.19.0",
        "compression": "1.7.4",
        "cron": "^1.8.2",
        "dotenv": "10.0.0",
        "errorhandler": "1.5.1",
        "express": "4.17.1",
        "express-flash": "0.0.2",
        "express-validator": "6.9.2",
        "fbgraph": "1.4.4",
        "joi": "^17.4.2",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "lusca": "1.6.1",
        "mongoose": "6.0.12",
        "nodemailer": "^6.7.0",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@types/async": "3.2.5",
        "@types/bcryptjs": "^2.4.2",
        "@types/bluebird": "3.5.33",
        "@types/body-parser": "1.19.0",
        "@types/chai": "4.2.14",
        "@types/compression": "1.7.0",
        "@types/cron": "^1.7.3",
        "@types/errorhandler": "1.5.0",
        "@types/eslint": "7.2.6",
        "@types/express": "4.17.11",
        "@types/express-flash": "0.0.2",
        "@types/jest": "27.0.2",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/lodash": "^4.14.170",
        "@types/lusca": "1.6.2",
        "@types/node": "16.11.6",
        "@types/nodemailer": "^6.4.4",
        "@types/supertest": "2.0.10",
        "@types/winston": "2.4.4",
        "@typescript-eslint/eslint-plugin": "5.2.0",
        "@typescript-eslint/parser": "5.2.0",
        "chai": "4.3.0",
        "eslint": "8.1.0",
        "jest": "^27.0.6",
        "nodemon": "^2.0.7",
